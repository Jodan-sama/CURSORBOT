{
  "name": "cursorbot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "build": "tsc",
    "start": "node dist/index.js",
    "bot": "node dist/bot/run.js",
    "test-order": "node dist/scripts/place-test-order.js",
    "test-order-poly": "node dist/scripts/place-test-order-polymarket.js",
    "claim-poly": "node dist/scripts/claim-polymarket.js",
    "b4-paper": "node dist/scripts/b4-paper.js",
    "spreads": "node dist/scripts/current-kalshi-spreads.js",
    "spreads:dev": "npx tsx src/scripts/current-kalshi-spreads.ts",
    "dev": "tsx watch src/index.ts",
    "test": "node --test dist/**/*.test.js"
  },
  "dependencies": {
    "@polymarket/clob-client": "^5.2.1",
    "@supabase/supabase-js": "^2.45.0",
    "dotenv": "^16.6.1",
    "ethers": "^6.16.0",
    "https-proxy-agent": "^7.0.6",
    "undici": "^6.21.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "patch-package": "^8.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
