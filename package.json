{
  "name": "cursorbot",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "scripts": {
    "postinstall": "patch-package",
    "build": "tsc",
    "start": "node dist/index.js",
    "bot": "node dist/bot/run.js",
    "test-order": "node dist/scripts/place-test-order.js",
    "test-order-poly": "node dist/scripts/place-test-order-polymarket.js",
    "claim-poly": "node dist/scripts/claim-polymarket.js",
    "emergency-off": "npx tsx src/scripts/set-emergency-off.ts off",
    "check-supabase": "npx tsx src/scripts/check-supabase.ts",
    "emergency-on": "npx tsx src/scripts/set-emergency-off.ts on",
    "check-safe-balance": "node dist/scripts/check-safe-balance.js",
    "transfer-safe-usdc": "node dist/scripts/transfer-safe-usdc.js",
    "b4-paper": "node dist/scripts/b4-paper.js",
    "b4-kalshi-46-50": "node dist/scripts/b4-kalshi-46-50.js",
    "b4-5m": "node dist/b4-5m/run.js",
    "b5": "node dist/b5-basket/run.js",
    "b5-spread": "node dist/b5-5m/spread-run.js",
    "b4-5m:backtest": "npx tsx src/b4-5m/backtest.ts",
    "b4-5m:download": "npx tsx src/b4-5m/download-candles.ts",
    "spreads": "node dist/scripts/current-kalshi-spreads.js",
    "spreads:dev": "npx tsx src/scripts/current-kalshi-spreads.ts",
    "dev": "tsx watch src/index.ts",
    "test": "node --test dist/**/*.test.js"
  },
  "dependencies": {
    "@polymarket/clob-client": "^5.2.1",
    "@safe-global/protocol-kit": "^6.1.2",
    "@safe-global/types-kit": "^3.0.0",
    "@supabase/supabase-js": "^2.45.0",
    "@types/ws": "^8.18.1",
    "dotenv": "^16.6.1",
    "ethers": "^6.16.0",
    "https-proxy-agent": "^7.0.6",
    "undici": "^6.21.0",
    "ws": "^8.19.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.0",
    "patch-package": "^8.0.1",
    "tsx": "^4.19.2",
    "typescript": "^5.7.2"
  }
}
